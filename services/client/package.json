{
  "name": "my-dev-space",
  "homepage": "https://github.com/apoclyps/my-dev-space",
  "version": "0.1.0",
  "private": false,
  "optionalDependencies": {
    "fsevents": "*"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "tailwind:css": "tailwind build src/css/tailwind.src.css -c tailwind.js -o src/css/tailwind.css",
